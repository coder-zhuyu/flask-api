<uwsgi>
  <pythonpath>.</pythonpath>
  <module>manage</module>
  <callable>app</callable>
  <http-socket>0.0.0.0:9096</http-socket>
  <!--<socket>/tmp/uwsgi.sock</socket>-->
  <listen>2048</listen>
  <master/>
  <processes>1</processes>
  <threads>10</threads>
  <disable-logging>true</disable-logging>
  <daemonize>./logs/uwsgi.log</daemonize>
  <log-maxsize>100000000</log-maxsize>
  <log-backupname>./logs/uwsgi.log.old</log-backupname>
  <pidfile>./logs/uwsgi.pid</pidfile>
</uwsgi>